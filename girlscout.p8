-- utils
bpal = {}
for i=1,16,1 do
	add(bpal,1)
end
function ospr(i,x,y,xblit,yblit,flipx,flipy)
	xblit=xblit or 1
	yblit=yblit or 1
	flipx=flipx or false
	flipy=flipy or false
	pal(bpal)
	palt(0,false)
	palt(14,true)
	for xx=x-1,x+1,1 do
	for yy=y-1,y+1,1 do
		spr(i,xx,yy,xblit,yblit,flipx,flipy)
	end
	end
	pal()
	palt(0,false)
	palt(14,true)
 spr(i,x,y,xblit,yblit,flipx,flipy)	
 pal()
end

function mgetp(x,y)
	return mget(x\8,y\8)
end

function fade_out()
	for i=0,80,5 do
		circfill(64+camx,64+camy,i,0)
		flip()
	end
end

function fade_in()
	for i=80,0,-5 do
		_draw()
		circfill(64+camx,64+camy,i,0)
		flip()
	end
end

function adder(f)
	val=flr(t()*60)%(f*2)
	if (val>f-1) return 1
	return 0
end
