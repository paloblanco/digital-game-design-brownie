-- mapping

function init_map()
	camera(128,0)
	
	box    = {}
	box.x  = 25
	box.y  = 14
	box.id = 1
	
	_update60 = update_map
	_draw = draw_map
	
end

function update_map()
	mousex,mousey = stat(32)-1+128,stat(33)-1
	
	if btn(5) then
		if mousey < 13*8 then
			put_tile()
		elseif mousex > 128 + 64 then
			get_tile()
		end
	end
end

function put_tile()
	if box.id==1 or box.id==16 then
		removeid(box.id)
	end
	mset(mousex\8,mousey\8,box.id)
end

function get_tile()
	tmouse = mget(mousex\8,mousey\8)
	if (tmouse == 0) return
	box.id=tmouse
	box.x = mousex\8
	box.y = mousey\8
end

function draw_map()
	cls()
	map()
	rect(128,104,255,127,7)
	circfill(mousex,mousey,2,7)
	if ((t()*5)%2>1) rect(box.x*8-1,box.y*8-1,box.x*8+8,box.y*8+8,7)
	if mousey\8>12 then
		mx = mousex-mousex%8
		my = mousey-mousey%8
		rect(mx-1,my-1,mx+8,my+8,6)
	end
end

function removeid(id)
	for xx=0,15,1 do
	for yy=0,12,1 do
		if mget(xx+16,yy)==id then
			mset(xx+16,yy,5)
		end
	end
	end
end

function copy_map()
	for xx=0,15,1 do
	for yy=0,12,1 do
		mset(xx,yy,mget(xx+16,yy))
	end
	end
end
