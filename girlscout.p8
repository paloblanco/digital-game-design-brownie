-- design:



function _init()
	poke(0x5f2d, 3) -- mouse
	poke(0x5f5c, 255) -- no key repeat	
	
	menuitem(1,"goto play mode",init_play)
	menuitem(2,"goto map mode",init_map)
		
	init_play()
end

function _update60()	
end

function _draw()
end

function init_play()
	camera()
	copy_map()
	
	p1 = {}
	p1.x = 0
	p1.y = 0
	
	cat = {}
	cat.x = 0
	cat.y = 0
	
	faceleft = false
	
	crawl_map()
	
	_update60 = update_play
	_draw = draw_play
end

function update_play()
	diag   = 1
	speed  = 0.7
	moving = false
	dx     = 0
	dy     = 0
	
	mousex,mousey = stat(32)-1,stat(33)-1
	
	lr = btn(0) or btn(1)
	ud = btn(2) or btn(3)
	if (lr and ud) diag = 0.71
	moving = lr or ud
	if btn(0) then
		dx = -speed*diag
		faceleft=true
	end
	if btn(1) then
		dx = speed*diag
		faceleft=false
	end
	if (btn(2)) dy = -speed*diag
	if (btn(3)) dy = speed*diag
	
	p1.x += dx
	p1.y += dy
	
	p1_bump()
end

function draw_play()
	cls()
	--adjust_cam()
	map()
	rect(0,104,127,127,7)
	scount = 1
	if (moving) scount = 1 + (t()*10)%2
	
	ospr(scount,p1.x,p1.y-8,1,2,faceleft)
	ospr(16,cat.x,cat.y)
	
	circfill(mousex,mousey,2,7)
end
