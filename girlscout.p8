-- girl scout cat rescue
-- rocco panella, 2024

function _init()
	poke(0x5f2d, 3) -- mouse
	poke(0x5f5c, 255) -- no key repeat	
	
	menuitem(1,"goto play mode",init_play)
	menuitem(2,"goto map mode",init_map)
	
	scout_ix = {1,3,33}
	cls(0)
	
	camx=0
	camy=0
	
	timestart=0
end

function _update60()
	timestart+=1
	 if (timestart>1) init_play()
end

function _draw()
	cls(0)
end

function init_play()
	fade_out()
	camx=0
	camy=0
	camera(camx,camy)
	copy_map()
	
	p1 = {}
	p1.x = 0
	p1.y = 0
	
	cat = {}
	cat.x = 0
	cat.y = 0
	
	pigs = {}
	
	camx=0
	camy=0
	
	faceleft = false
	
	crawl_map()
	
	_update60 = update_play
	_draw = draw_play
	
	fade_in()
end

function update_play()
	diag   = 1
	speed  = 0.7
	moving = false
	dx     = 0
	dy     = 0
	
	mousex,mousey = stat(32)-1,stat(33)-1
	
	lr = btn(0) or btn(1)
	ud = btn(2) or btn(3)
	if (lr and ud) diag = 0.71
	moving = lr or ud
	if btn(0) then
		dx = -speed*diag
		faceleft=true
	end
	if btn(1) then
		dx = speed*diag
		faceleft=false
	end
	if (btn(2)) dy = -speed*diag
	if (btn(3)) dy = speed*diag
	
	p1.x += dx
	p1.y += dy
	
	p1_bump()
	
	for p in all(pigs) do
		p.x += p.dx
		if p.dx>0 then
			if fget(mgetp(p.x+7,p.y),0) or p.x>120 then
				p.dx *= -1
			end
		end
		if p.dx<0 then
			if fget(mgetp(p.x+1,p.y),0) or p.x < 0 then
				p.dx *= -1
			end
		end
		p.y += p.dy
		if p.dy>0 then
			if fget(mgetp(p.x,p.y+7),0) or p.y>12*8 then
				p.dy *= -1
			end
		end
		if p.dy<0 then
			if fget(mgetp(p.x,p.y+1),0) or p.y < 0 then
				p.dy *= -1
			end
		end
	end
	
	check_cat()
	check_pigs()
	
	on_make_button = false
	check_make_button()
end

function check_make_button()
	if (not fget(mgetp(mousex,mousey),2)) return
	on_make_button = true
	if btnp(5) then
		init_map()
	end
end

function draw_play()
	cls()
	--adjust_cam()
	map()
	rect(0,104,127,127,7)
	scount = 1
	if (moving) scount = 1 + adder(6)
	
	ospr(16,cat.x,cat.y)
	for p in all(pigs) do
		ospr(p.ix+adder(8)*16,p.x,p.y)
	end
	ospr(scount,p1.x,p1.y-8,1,2,faceleft)
	
	
	if (on_make_button and (t()*5)%2>1) rect(7,14*8-1,24,15*8)
	
	circfill(mousex,mousey,2,7)
end

function check_cat()
	if collide(p1,cat) then
		win()
	end
end

function check_pigs()
	for p in all(pigs) do
		if collide(p1,p) then
			lose()
		end
	end
end

function win()
	init_play()
end

function lose()
	init_play()
end

function collide(a,b,r)
	r=r or 6
	if abs(a.x-b.x) <= r and
	 abs(a.y-b.y) <= r then
	 return true
	end
	return false
end
