-- gameplay

function crawl_map()
	for xx=0,15,1 do
	for yy=0,13,1 do
		tile=mget(xx,yy)
		if tile==1 then
			p1.x=xx*8
			p1.y=yy*8
			mset(xx,yy,5)			
		elseif tile==16 then
			cat.x=xx*8
			cat.y=yy*8
			mset(xx,yy,5)			
		elseif tile==32 then
			local p = {}
			p.x  = xx*8
			p.y  = yy*8
			p.dx = 0
			p.dy = .5
			p.ix = 32		
			add(pigs,p)
			mset(xx,yy,5)
		end
	end
	end
end

function adjust_cam()
	camy=0
	camx = max(0,p1.x-64)
	camx = min(128,camx)
	camera(camx,camy)
end

function p1_bump()
	while p1_bumpup() do
		p1.y = flr(p1.y + 1)
	end
	if (p1_bumpdown()) p1.y = flr(p1.y)
	while p1_bumpdown() do
		p1.y = flr(p1.y - 1)
	end
	while p1_bumpleft() do
		p1.x = flr(p1.x + 1)
	end
	if (p1_bumpright()) p1.x = flr(p1.x)
	while p1_bumpright() do
		p1.x = flr(p1.x - 1)
	end
end

function mgetp(x,y)
	return mget(x\8,y\8)
end

function p1_bumpup()
	if (p1.y < 0) return true
	if (dy > 0) return false
	bump = fget(mgetp(p1.x+2,p1.y+1),0) or 
		fget(mgetp(p1.x+6,p1.y+1),0)
	return bump
end

function p1_bumpdown()
	if (p1.y+15>111) return true
	if (dy < 0) return false
	bump = fget(mgetp(p1.x+2,p1.y+7),0) or 
		fget(mgetp(p1.x+6,p1.y+7),0)
	return bump
end

function p1_bumpleft()
	if (p1.x < 0) return true
	if (dx > 0) return false
	bump = fget(mgetp(p1.x+1,p1.y+1),0) or 
		fget(mgetp(p1.x+1,p1.y+6),0)
	return bump
end

function p1_bumpright()
	if (p1.x+7)>127 return true
	if (dx < 0) return false
	bump = fget(mgetp(p1.x+7,p1.y+1),0) or 
		fget(mgetp(p1.x+7,p1.y+6),0)
	return bump
end
